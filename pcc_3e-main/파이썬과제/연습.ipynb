    "class Number:\n",
    "  def __init__(self, *args):\n",
    "    self.namx = args\n",
    "\n",
    "  def ns(self): \n",
    "    result = []\n",
    "    for i in self.namx:\n",
    "      if i%2==0:\n",
    "        result.append(\"Even\")\n",
    "      else :\n",
    "        result.append(\"Odd\")\n",
    "    return result\n",
    "\n",
    "n = Number(5, 2, -3, -4, 0)\n",
    "print(n.ns())"
   ]
  }
